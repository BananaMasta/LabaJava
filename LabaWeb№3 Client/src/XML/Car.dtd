<?xml version="1.0" encoding="UTF-8" ?>
        <!ELEMENT Carlist (carnumber*)>
        <!ELEMENT carnumber (car*, AmountOfMaxSpeed?)>
        <!ATTLIST carnumber
                number CDATA #REQUIRED
                >

        <!ELEMENT car (carprice, carstats)>
        <!ATTLIST car
                name CDATA #REQUIRED
                >

        <!ELEMENT carprice EMPTY>
        <!ATTLIST carprice
                price CDATA #REQUIRED
                >

        <!ELEMENT carstats EMPTY>
        <!ATTLIST carstats
                maxspeed CDATA #REQUIRED
                >
        <!ELEMENT AmountOfMaxSpeed (#PCDATA)>